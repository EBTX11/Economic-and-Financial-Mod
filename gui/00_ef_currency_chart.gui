types ef_bp_panel_types
{
	type ef_bp_money_supply_piechart = chart {
		blockoverride "datamodel" {
			datamodel = "[GetPlayer.MakeScope.GetList('money_supply_variable_list_ordered_1')]"
		}
		blockoverride "heading" {
			text = "EF_BP_CHART_MONEY_SUPPLY_HEADING_TITLE"
		}
		blockoverride "tooltip" {
			#tooltip = "EF_BP_CHART_MONEY_SUPPLY_TOOLTIP"
			tooltip = "MONEY_VALUE"
		}
		blockoverride "pieslice" {
			value = "[Scope.GetCountry.MakeScope.ScriptValue('total_money_supply')|D=+]"
		}
		blockoverride "color" {
			color = "[Scope.GetCountry.GetMapColor]"
		}

		blockoverride "leftside_info" {
			raw_text = "[Scope.GetCountry.GetCustom('currency_good')] [Scope.GetCountry.GetCustom('currency_name')]"
		}
		blockoverride "rightside1_info" {
			raw_text = "#bold #gold [Scope.GetCountry.MakeScope.ScriptValue('total_money_supply')|D=+] #!#!"

		}

		blockoverride "rightside2_info" {
			raw_text = ""
		}
		blockoverride "pie_item_goto_button" {
			button = {
				using = clean_button
				size = { 100% 100% }
			}
		}
		blockoverride "piechartsize" {
			size = { 200 200 }
		}
		blockoverride "minimumsize" {
			minimumsize = { 100 -1 }
		}
		blockoverride "maxverticalslots" {
			maxverticalslots = 10
		}
	}

