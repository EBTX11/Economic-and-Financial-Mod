					text = "MARKET_PANEL_BALANCE"
					autoresize = yes
					parentanchor = hcenter
				}

				textbox = {
					visible = "[LessThan_int32(FixedPointToInt(Goods.GetMarketBuyAndSellOrdersBalance), '(int32)0')]"
