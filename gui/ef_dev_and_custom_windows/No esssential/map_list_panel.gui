				size = { 55 40 }
				onclick = "[MapListPanel.Sort('rank')]"

				icon = {
					texture = "gfx/interface/buttons/sort_button_icons/sort_icon_rank.dds"
					size = { 25 25 }
					parentanchor = center
					alwaystransparent = yes
				}

				tooltip = "SORT_BY_RANK"

				blockoverride "selected_visibility" {
					visible = no
