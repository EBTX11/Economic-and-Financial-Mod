
default_popup = {
	name = "ef_custom_windows"
	resizable = yes
	layer = top
	movable = no
	
	size = { 500 250 }
	
	using = Window_Background
	
	parentanchor = center

	blockoverride "window_header_name" {
		text = "ef_custom_windows"
	}
	blockoverride "header_back_button_visibility" {
		visible = no
	}
	blockoverride "header_close_button_visibility" {
		visible = yes
	}
	blockoverride "header_close_button" {
		onclick = "[ExecuteConsoleCommand( Select_CString( GetVariableSystem.Exists('my_ef_custom_windows'), 'gui.ClearWidgets ef_custom_windows', 'gui.createwidget gui/ef_custom_windows/ef_custom_windows.gui ef_custom_windows' ) )]"
		onclick = "[GetVariableSystem.Toggle('my_ef_custom_windows')]"
	}
	
	blockoverride "entire_scrollarea" {
		widget = {

			size = { 600 100 }

			using = default_list_position
			direction = vertical

			allow_outside = yes
			ignoreinvisible = no

			
			flowcontainer = {
				layoutpolicy_horizontal = expanding  
				allow_outside = yes
				ignoreinvisible = no
				parentanchor = left|nobaseline
				direction = horizontal

				position  = { 0 0 }

				spacing = 5

				#Panel_1
				button_icon_round = {
					name = "Panel_1"
					size = { 33 33 }


					onclick = "[ExecuteConsoleCommand( Select_CString( GetVariableSystem.Exists('my_ef_custom_windows'), 'gui.ClearWidgets ef_custom_windows', 'gui.createwidget gui/ef_custom_windows/ef_custom_windows.gui ef_custom_windows' ) )]"
					onclick = "[GetVariableSystem.Toggle('my_ef_custom_windows')]"

					sing = shimmer
					position = { 100 60 }
					textbox = {
						text = "1"
						align = center|nobaseline
						default_format = "#white #bold"
						autoresize = yes
						fontsize = 25
						allow_outside = no
						ignoreinvisible = yes
						margin_left = 10
					}
				}

				#####################################################################################################################################################
