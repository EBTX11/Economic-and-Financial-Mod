#panel_9
default_popup = {
	name = "panel_9"
	resizable = no
	layer = top
	movable = yes
	
	size = { 420 550 }
	
	using = Window_Background
	
	parentanchor = center

	blockoverride "window_header_name" {
		text = "variable_test_var_2"
	}
	blockoverride "header_back_button_visibility" {
		visible = no
	}
	blockoverride "header_close_button_visibility" {
		visible = yes
	}
	blockoverride "header_close_button" {
		onclick = "[ExecuteConsoleCommand( Select_CString( GetVariableSystem.Exists('panel_9'), 'gui.ClearWidgets panel_9', 'gui.createwidget gui/ef_custom_windows/ef_custom_windows.gui panel_9' ) )]"
		onclick = "[GetVariableSystem.Toggle('panel_9')]"
	}

	blockoverride "entire_scrollarea" {
		widget = {

			size = { 600 900 }

			using = default_list_position
			direction = vertical

			allow_outside = yes
			ignoreinvisible = no


