template visible_if_ownership_can_be_split
{
	# The buildings ownership can be split between multiple entities:
	# It is not a Manor House, Financial District (GetBuildingAssets)
	# It is not a Government Building. 
	# It is not a Urban Center
	# Or it is simply a level 0 building
	visible = "[And( Not(Building.IsUrbanCenter), And( And( IsDataModelEmpty(Building.GetBuildingAssets), Not(Building.IsGovernmentFunded) ), NotEqualTo_int32(Building.GetExpansionLevel, '(int32)0')))]"
}

template visible_if_ownership_always_workforce
{
	visible = "[Building.GetBuildingType.IsAlwaysSelfOwning]"
}

template building_size_choose_alpha
{
	# show building size label without any transparency if either:
	# - it is built,
	# - construction is queued
	# - it has "interesting" max level, i.e. uses arable land or a resource cap
	alpha = "[TransparentIfFalse( Or( Building.GetBuildingType.IsCappedBuilding, Or( GreaterThan_int32(Building.GetExpansionLevel, '(int32)0'), Building.HasConstructionQueued )))]"
}
