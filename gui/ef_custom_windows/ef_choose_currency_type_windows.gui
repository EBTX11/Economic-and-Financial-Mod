#ef_choose_currency_type_windows
default_popup = {
	name = "ef_choose_currency_type_windows"
	resizable = no
	layer = top
	movable = yes
	
	size = { 420 550 }
	
	using = Window_Background
	
	parentanchor = center

	blockoverride "window_header_name" {
		text = "variable_test_var_2"
	}
	blockoverride "header_back_button_visibility" {
		visible = no
	}
	blockoverride "header_close_button_visibility" {
		visible = yes
	}
	blockoverride "header_close_button" {
		onclick = "[ExecuteConsoleCommand( Select_CString( GetVariableSystem.Exists('ef_choose_currency_type_windows'), 'gui.ClearWidgets ef_choose_currency_type_windows', 'gui.createwidget gui/ef_custom_windows/ef_custom_windows.gui ef_choose_currency_type_windows' ) )]"
		onclick = "[GetVariableSystem.Toggle('ef_choose_currency_type_windows')]"
	}

