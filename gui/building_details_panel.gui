						parentanchor = center
					}
					align = left|nobaseline
					onclick = "[Building.SortBuildings('earnings_per_employee')]"
				}

				sort_button = {
					size = { 70 30 }
					text = "DIVIDENDS"
					align = left|nobaseline
					onclick = "[Building.SortBuildings('dividend')]"
