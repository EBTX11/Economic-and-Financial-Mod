					visible = "[Not(Building.IsBuildingType('building_company_headquarter'))]" #todo - PRCAL-38246 Make sure this function works for company HQs, does not seem to be correct atm, but it works for other building types.
					size = { 115 115 }
					onrightclick = "[RightClickMenuManager.ShowForBuilding(Building.AccessSelf)]"

					tooltipwidget = {
						FancyTooltip_Building = {}
					}

					icon = {
						alpha = "[TransparentIfFalse(Building.IsActive)]"
