				#####################################################################################################################################################
				#####################################################################################################################################################
				#####################################################################################################################################################
				#####################################################################################################################################################
				#####################################################################################################################################################

				#panel_2
				button_icon_round = {
					name = "panel_2"
					size = { 33 33 }
		
					onclick = "[ExecuteConsoleCommand( Select_CString( GetVariableSystem.Exists('panel_2'), 'gui.ClearWidgets panel_2', 'gui.createwidget gui/ef_custom_windows/ef_custom_windows.gui panel_2' ) )]"
					onclick = "[GetVariableSystem.Toggle('panel_2')]"
		
					sing = shimmer
					position = { 140 60 }
					textbox = {
						text = "_2"
						align = center|nobaseline
						default_format = "#white #bold"
						autoresize = yes
						fontsize = 25
						allow_outside = no
						ignoreinvisible = yes
						margin_left = 10
					}
				}
				#panel_3
				button_icon_round = {
					name = "panel_3"
					size = { 33 33 }

					onclick = "[ExecuteConsoleCommand( Select_CString( GetVariableSystem.Exists('panel_3'), 'gui.ClearWidgets panel_3', 'gui.createwidget gui/ef_custom_windows/ef_custom_windows.gui panel_3' ) )]"
